[Unit]
Description=FinEdge AI Edge Predictor
After=network.target

[Service]
Type=simple
User=pi
WorkingDirectory=/home/pi/Projects/AI/ChatGPT/finedge
ExecStart=/home/pi/Projects/AI/ChatGPT/finedge/venv/bin/python3 app.py
Restart=always
RestartSec=10
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
